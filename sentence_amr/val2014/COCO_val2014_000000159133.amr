# ::tok A toilet , with a roll of toilet paper and a children 's Hanukkah book on the counter next to it .
# ::node	t	toilet	1-2
# ::node	r	roll	5-6
# ::node	p	paper	8-9
# ::node	a	and	9-10
# ::node	c	child	11-12
# ::node	b	book	13-14
# ::node	n	name	13-14
# ::node	0	Hanukkah	13-14
# ::node	b2	book	14-15
# ::node	c2	counter	17-18
# ::node	n2	next-to	18-19
# ::root	t	toilet
# ::edge	roll	consist-of	paper	r	p	
# ::edge	paper	mod	toilet	p	t	
# ::edge	and	op1	roll	a	r	
# ::edge	toilet	part	and	t	a	
# ::edge	book	mod	child	b	c	
# ::edge	book	name	name	b	n	
# ::edge	name	op1	Hanukkah	n	0	
# ::edge	book	mod	book	b2	b	
# ::edge	and	op2	book	a	b2	
# ::edge	and	location	counter	a	c2	
# ::edge	counter	location-of	next-to	c2	n2	
# ::edge	next-to	op1	toilet	n2	t	
(t / toilet
    :part (a / and
        :location (c2 / counter
            :location-of (n2 / next-to
                :op1 t))
        :op1 (r / roll
            :consist-of (p / paper
                :mod t))
        :op2 (b2 / book
            :mod (b / book
                :name (n / name
                    :op1 "Hanukkah")
                :mod (c / child)))))
