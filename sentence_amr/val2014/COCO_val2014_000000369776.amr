# ::tok A stop sign in a foreign language with the letters " ALTO " on it .
# ::node	s2	stop-01	1-2
# ::node	s	sign	2-3
# ::node	f	foreign	5-6
# ::node	l	language	6-7
# ::node	l2	letter	9-10
# ::node	n	name	11-12
# ::node	0	Al	11-12
# ::root	s	sign
# ::edge	sign	purpose	stop-01	s	s2	
# ::edge	language	mod	foreign	l	f	
# ::edge	sign	mod	language	s	l	
# ::edge	sign	location-of	letter	s	l2	
# ::edge	letter	name	name	l2	n	
# ::edge	name	op1	Al	n	0	
(s / sign
    :purpose (s2 / stop-01)
    :location-of (l2 / letter
        :name (n / name
            :op1 "Al"))
    :mod (l / language
        :mod (f / foreign)))
