(c2 / clock
    :ARG0-of (h / have-03
        :ARG1 (n2 / number
            :mod (c3 / country
                :name (n / name
                    :op1 "Rome"))))
    :ARG1-of (s / set-01
        :ARG2 (d / date-entity
            :time "1:00"))
    :mod (s2 / steeple)
    :mod (c / church))
