# ::tok A building with a clock mounted on the front of it
# ::node	b	building	1-2
# ::node	h	have-03	2-3
# ::node	c	clock	4-5
# ::node	m	mount-03	5-6
# ::node	f	front	8-9
# ::root	b	building
# ::edge	building	ARG0-of	have-03	b	h	
# ::edge	have-03	ARG1	clock	h	c	
# ::edge	clock	ARG1-of	mount-03	c	m	
# ::edge	mount-03	location	front	m	f	
# ::edge	building	part	front	b	f	
(b / building
    :ARG0-of (h / have-03
        :ARG1 (c / clock
            :ARG1-of (m / mount-03
                :location (f / front))))
    :part f)
