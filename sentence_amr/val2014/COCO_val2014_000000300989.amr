# ::tok Clock tower at the Port of San Francisco .
# ::node	t	tower	0-1
# ::node	t2	tower	1-2
# ::node	p	port	4-5
# ::node	n	name	4-5
# ::node	1	Port	4-5
# ::node	3	of	5-6
# ::node	2	San	6-7
# ::node	0	Francisco	7-8
# ::root	t2	tower
# ::edge	tower	domain	tower	t2	t	
# ::edge	tower	location	port	t2	p	
# ::edge	port	name	name	p	n	
# ::edge	name	op1	Port	n	1	
# ::edge	name	op2	of	n	3	
# ::edge	name	op3	San	n	2	
# ::edge	name	op4	Francisco	n	0	
(t2 / tower
    :domain (t / tower)
    :location (p / port
        :name (n / name
            :op1 "Port"
            :op2 "of"
            :op3 "San"
            :op4 "Francisco")))


# ::tok lit against the night sky .
# ::node	l	light-04	0-1
# ::node	d	date-entity	3-4
# ::node	n	night	3-4
# ::node	s	sky	4-5
# ::root	l	light-04
# ::edge	date-entity	dayperiod	night	d	n	
# ::edge	sky	time	date-entity	s	d	
# ::edge	light-04	prep-against	sky	l	s	
(l / light-04
    :prep-against (s / sky
        :time (d / date-entity
            :dayperiod (n / night))))
