(p2 / plate
    :quant (b / bunch)
    :consist-of (f2 / food
        :example (a / and
            :op1 (f / fish)
            :op2 (p3 / pork)
            :op3 (w / watermelon)
            :op4 (s / salad
                :mod (p / pasta))
            :op5 (s2 / sauce
                :mod (c / cocktail)))))
