# ::tok A street clock that has been weathered to a mint green color
# ::node	s	street	1-2
# ::node	c	clock	2-3
# ::node	w	weather-01	6-7
# ::node	m	mint	9-10
# ::node	g	green-02	10-11
# ::node	c2	color	11-12
# ::root	c	clock
# ::edge	clock	mod	street	c	s	
# ::edge	clock	ARG0-of	weather-01	c	w	
# ::edge	color	ARG1-of	green-02	c2	g	
# ::edge	color	mod	mint	c2	m	
# ::edge	weather-01	ARG1	color	w	c2	
(c / clock
    :ARG0-of (w / weather-01
        :ARG1 (c2 / color
            :ARG1-of (g / green-02)
            :mod (m / mint)))
    :mod (s / street))
