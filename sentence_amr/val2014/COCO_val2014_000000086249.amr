# ::tok An infant being held in a person 's arms an holding a toy in
# ::node	i	infant	1-2
# ::node	h	hold-01	3-4
# ::node	p	person	6-7
# ::node	a2	arm	8-9
# ::node	a	and	9-10
# ::node	h2	hold-01	10-11
# ::node	t	toy	12-13
# ::root	a	and
# ::edge	hold-01	ARG1	infant	h	i	
# ::edge	arm	part-of	person	a2	p	
# ::edge	hold-01	ARG2	arm	h	a2	
# ::edge	and	op1	hold-01	a	h	
# ::edge	and	op2	hold-01	a	h2	
# ::edge	hold-01	ARG0	infant	h2	i	
# ::edge	hold-01	ARG1	toy	h2	t	
(a / and
    :op1 (h / hold-01
        :ARG1 (i / infant)
        :ARG2 (a2 / arm
            :part-of (p / person)))
    :op2 (h2 / hold-01
        :ARG0 i
        :ARG1 (t / toy)))


