# ::node	s3	sub	2-3
# ::node	s	sandwich	3-4
# ::node	w	wheat	5-6
# ::node	b	bread	6-7
# ::node	p	pickle	9-10
# ::node	s2	spear	10-11
# ::root	s	sandwich
# ::edge	sandwich	mod	sub	s	s3	
# ::edge	sandwich	mod	turkey	s	t	
# ::edge	bread	consist-of	wheat	b	w	
# ::edge	sandwich	location	bread	s	b	
# ::edge	spear	consist-of	pickle	s2	p	
