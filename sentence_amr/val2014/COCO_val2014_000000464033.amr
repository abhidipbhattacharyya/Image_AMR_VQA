# ::tok An ornate tower in a Gothic or Gothic Revival style of architecture .
# ::node	o3	ornate	1-2
# ::node	t	tower	2-3
# ::node	0	Gothic	5-6
# ::node	o2	or	6-7
# ::node	n	name	7-8
# ::node	o	Gothic	7-8
# ::node	1	Revival	8-9
# ::node	s	style	9-10
# ::node	s2	style	9-10
# ::node	a	architecture	11-12
# ::root	t	tower
# ::edge	tower	mod	ornate	t	o3	
# ::edge	name	op1	Gothic	n	0	
# ::edge	name	op2	Revival	n	1	
# ::edge	style	mod	name	s	n	
# ::edge	tower	mod	style	t	s	
# ::edge	style	name	name	s2	n	
# ::edge	or	op2	style	o2	s2	
# ::edge	or	mod	architecture	o2	a	
# ::edge	tower	rel	or	t	o2	
# ::edge	tower	rel	Gothic	t	o	
(t / tower
    :mod (o3 / ornate)
    :mod (s / style
        :mod (n / name
            :op1 "Gothic"
            :op2 "Revival"))
    :rel (o2 / or
        :mod (a / architecture)
        :op2 (s2 / style
            :name n))
    :rel (o / Gothic))
