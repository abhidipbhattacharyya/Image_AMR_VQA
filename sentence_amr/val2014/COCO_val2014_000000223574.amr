# ::tok Traffic on the road .
# ::node	t	traffic	0-1
# ::node	r	road	3-4
# ::root	t	traffic
# ::edge	traffic	location	road	t	r	
(t / traffic
    :location (r / road))


# ::tok Many traffic lights are seen controlling the traffic .
# ::node	m	many	0-1
# ::node	t	traffic	1-2
# ::node	l	light	2-3
# ::node	s	see-01	4-5
# ::node	c	control-01	5-6
# ::node	t2	traffic	7-8
# ::root	s	see-01
# ::edge	light	mod	traffic	l	t	
# ::edge	light	quant	many	l	m	
# ::edge	see-01	ARG1	control-01	s	c	
# ::edge	control-01	ARG0	light	c	l	
# ::edge	control-01	ARG1	traffic	c	t2	
(s / see-01
    :ARG1 (c / control-01
        :ARG0 (l / light
            :quant (m / many)
            :mod (t / traffic))
        :ARG1 (t2 / traffic)))
