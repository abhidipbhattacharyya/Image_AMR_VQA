# ::tok A stop sign with words written on it with a black marker .
# ::node	s2	stop-01	1-2
# ::node	s	sign	2-3
# ::node	h	have-03	3-4
# ::node	w	word	4-5
# ::node	w2	write-01	5-6
# ::node	b	black-04	10-11
# ::node	m	marker	11-12
# ::root	s	sign
# ::edge	sign	purpose	stop-01	s	s2	
# ::edge	sign	ARG0-of	have-03	s	h	
# ::edge	have-03	ARG1	word	h	w	
# ::edge	word	ARG1-of	write-01	w	w2	
# ::edge	write-01	location	sign	w2	s	
# ::edge	marker	ARG1-of	black-04	m	b	
# ::edge	write-01	instrument	marker	w2	m	
(s / sign
    :purpose (s2 / stop-01)
    :ARG0-of (h / have-03
        :ARG1 (w / word
            :ARG1-of (w2 / write-01
                :instrument (m / marker
                    :ARG1-of (b / black-04))
                :location s))))
