# ::tok An ornamental standing clock is at the foreground of a row of houses .
# ::node	o	ornamental	1-2
# ::node	s	stand-01	2-3
# ::node	c	clock	3-4
# ::node	f	foreground	7-8
# ::node	r	row	10-11
# ::node	h	house	12-13
# ::root	c	clock
# ::edge	clock	ARG2-of	stand-01	c	s	
# ::edge	clock	mod	ornamental	c	o	
# ::edge	clock	location	foreground	c	f	
# ::edge	foreground	part-of	row	f	r	
# ::edge	row	consist-of	house	r	h	
(c / clock
    :ARG2-of (s / stand-01)
    :location (f / foreground
        :part-of (r / row
            :consist-of (h / house)))
    :mod (o / ornamental))
