# ::node	l	look-02	5-6
# ::node	c	clock	6-7
# ::node	m	middle	9-10
# ::node	n	neighborhood	12-13
# ::root	c	clock
# ::edge	look-02	ARG1	old	l	o	
# ::edge	clock	ARG0-of	look-02	c	l	
# ::edge	clock	location	middle	c	m	
# ::edge	middle	part-of	neighborhood	m	n	
(c / clock
    :ARG0-of (l / look-02
        :ARG1 (o / old))
    :location (m / middle
        :part-of (n / neighborhood)))
