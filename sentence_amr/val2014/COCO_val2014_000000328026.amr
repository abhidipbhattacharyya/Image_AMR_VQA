# ::node	h	hold-01	2-3
# ::node	t	tie	4-5
# ::node	t2	tie	4-5
# ::node	n	next-to	5-6
# ::node	c	counter	8-9
# ::node	l	lot	10-11
# ::root	m	man
# ::edge	man	ARG0-of	hold-01	m	h	
# ::edge	hold-01	ARG1	tie	h	t	
# ::edge	hold-01	location	next-to	h	n	
# ::edge	next-to	op1	counter	n	c	
# ::edge	tie	quant	lot	t2	l	
