# ::node	s	station	2-3
# ::node	h	have-03	3-4
# ::node	0	2	4-5
# ::node	w	watch	5-6
# ::node	s2	suspend-02	6-7
# ::node	c	ceiling	9-10
# ::node	d	date-entity	11-12
# ::node	n	night	11-12
# ::root	s	station
# ::edge	station	mod	train	s	t	
