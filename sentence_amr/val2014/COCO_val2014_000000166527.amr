(s / station
    :ARG0-of (h / have-03
        :ARG1 (w / watch
            :quant 2
            :ARG1-of (s2 / suspend-02
                :ARG2 (c / ceiling)
                :time (d / date-entity
                    :dayperiod (n / night)))))
    :mod (t / train)
    :part c)
