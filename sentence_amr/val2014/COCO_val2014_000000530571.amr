# ::tok a clock on the outside of a building saying it is a little after 5 o clock
# ::node	c	clock	1-2
# ::node	o	outside	4-5
# ::node	b	building	7-8
# ::node	s	say-01	8-9
# ::node	l	little	12-13
# ::node	a	after	13-14
# ::node	d	date-entity	14-15
# ::node	0	17:00	14-15
# ::root	c	clock
# ::edge	clock	location	outside	c	o	
# ::edge	outside	part-of	building	o	b	
# ::edge	clock	ARG0-of	say-01	c	s	
# ::edge	after	quant	little	a	l	
# ::edge	say-01	ARG1	after	s	a	
# ::edge	after	op1	date-entity	a	d	
# ::edge	date-entity	time	17:00	d	0	
(c / clock
    :ARG0-of (s / say-01
        :ARG1 (a / after
            :quant (l / little)
            :op1 (d / date-entity
                :time "17:00")))
    :location (o / outside
        :part-of (b / building)))
