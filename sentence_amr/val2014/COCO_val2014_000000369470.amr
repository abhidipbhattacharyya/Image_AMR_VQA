# ::tok A row of parking spaces and parking meters in a downtown area
# ::node	r	row	1-2
# ::node	p	park-01	3-4
# ::node	s	space	4-5
# ::node	a	and	5-6
# ::node	m	meter	7-8
# ::node	d	downtown	10-11
# ::node	a2	area	11-12
# ::root	r	row
# ::edge	space	purpose	park-01	s	p	
# ::edge	and	op1	space	a	s	
# ::edge	row	consist-of	and	r	a	
# ::edge	and	op2	meter	a	m	
# ::edge	meter	mod	park-01	m	p	
# ::edge	area	mod	downtown	a2	d	
# ::edge	row	location	area	r	a2	
(r / row
    :consist-of (a / and
        :op1 (s / space
            :purpose (p / park-01))
        :op2 (m / meter
            :mod p))
    :location (a2 / area
        :mod (d / downtown)))
