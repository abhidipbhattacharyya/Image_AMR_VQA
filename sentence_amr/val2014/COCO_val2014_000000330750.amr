# ::tok A bench at the top of steps on the side of a grassy hill next to a large building .
# ::node	b	bench	1-2
# ::node	t	top	4-5
# ::node	s2	step-01	6-7
# ::node	s	side	9-10
# ::node	g	grass	12-13
# ::node	h	hill	13-14
# ::node	n	next-to	14-15
# ::node	l	large	17-18
# ::node	b2	building	18-19
# ::root	b	bench
# ::edge	bench	location	top	b	t	
# ::edge	top	mod	step-01	t	s2	
# ::edge	step-01	location	side	s2	s	
# ::edge	hill	consist-of	grass	h	g	
# ::edge	side	part-of	hill	s	h	
# ::edge	step-01	location	next-to	s2	n	
# ::edge	building	mod	large	b2	l	
# ::edge	next-to	op1	building	n	b2	
(b / bench
    :location (t / top
        :mod (s2 / step-01
            :location (s / side
                :part-of (h / hill
                    :consist-of (g / grass)))
            :location (n / next-to
                :op1 (b2 / building
                    :mod (l / large))))))
