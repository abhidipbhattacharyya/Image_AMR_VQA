# ::tok Tilted shot of a clock tower attached to a building .
# ::node	t	tint-01	0-1
# ::node	s	shot	1-2
# ::node	c	clock	4-5
# ::node	t2	tower	5-6
# ::node	a	attach-01	6-7
# ::node	b	building	9-10
# ::root	s	shot
# ::edge	shot	ARG1-of	tint-01	s	t	
# ::edge	tower	mod	clock	t2	c	
# ::edge	shot	topic	tower	s	t2	
# ::edge	tower	ARG1-of	attach-01	t2	a	
# ::edge	attach-01	ARG2	building	a	b	
(s / shot
    :ARG1-of (t / tint-01)
    :topic (t2 / tower
        :ARG1-of (a / attach-01
            :ARG2 (b / building))
        :mod (c / clock)))
