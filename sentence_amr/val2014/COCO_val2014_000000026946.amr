# ::tok A giraffe 's neck and head tower over a bar in its zoo house
# ::node	g	giraffe	1-2
# ::node	n	neck	3-4
# ::node	a	and	4-5
# ::node	h	head	5-6
# ::node	t	tower	6-7
# ::node	o	over	7-8
# ::node	b	bar	9-10
# ::node	z	zoo	12-13
# ::node	h2	house	13-14
# ::root	t	tower
# ::edge	and	op1	neck	a	n	
# ::edge	and	part-of	giraffe	a	g	
# ::edge	and	op2	head	a	h	
# ::edge	tower	domain	and	t	a	
# ::edge	tower	location	over	t	o	
# ::edge	over	op1	bar	o	b	
# ::edge	house	mod	zoo	h2	z	
# ::edge	bar	location	house	b	h2	
# ::edge	house	poss	giraffe	h2	g	
(t / tower
    :domain (a / and
        :op1 (n / neck)
        :op2 (h / head)
        :part-of (g / giraffe))
    :location (o / over
        :op1 (b / bar
            :location (h2 / house
                :mod (z / zoo)
                :poss g))))
