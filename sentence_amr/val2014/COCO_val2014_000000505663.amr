(a / and
    :op1 (c / clock
        :ARG0-of (h / have-03
            :ARG1 (n2 / numerical-quantity
                :ARG2-of (s / sign-01)
                :mod (c2 / country
                    :name (n / name
                        :op1 "Rome")))
            :location c))
    :op2 (m / mount-03
        :ARG1 c
        :location (w / wall)))
