# ::tok A train on the tracks under a walkway from one building to the next
# ::node	t2	train	1-2
# ::node	t	track	4-5
# ::node	u	under	5-6
# ::node	w	walkway	7-8
# ::node	o	one	9-10
# ::node	b	building	10-11
# ::node	b2	building	10-11
# ::node	n	next	13-14
# ::root	t2	train
# ::edge	train	location	track	t2	t	
# ::edge	train	location	under	t2	u	
# ::edge	under	op1	walkway	u	w	
# ::edge	building	mod	one	b	o	
# ::edge	walkway	source	building	w	b	
# ::edge	walkway	source	building	w	b2	
# ::edge	building	mod	next	b2	n	
(t2 / train
    :location (t / track)
    :location (u / under
        :op1 (w / walkway
            :source (b / building
                :mod (o / one))
            :source (b2 / building
                :mod (n / next)))))
