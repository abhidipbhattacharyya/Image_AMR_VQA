# ::tok a bird holding an object in
# ::node	b	bird	1-2
# ::node	h	hold-01	2-3
# ::node	o	object	4-5
# ::root	b	bird
# ::edge	bird	ARG0-of	hold-01	b	h	
# ::edge	hold-01	ARG1	object	h	o	
(b / bird
    :ARG0-of (h / hold-01
        :ARG1 (o / object)))


