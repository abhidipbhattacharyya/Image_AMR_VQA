# ::tok A steam engine on a railway turntable with houses in the background .
# ::node	s	steam	1-2
# ::node	e	engine	2-3
# ::node	r	railway	5-6
# ::node	t	turntable	6-7
# ::node	h	house	8-9
# ::node	b	background	11-12
# ::root	e	engine
# ::edge	engine	mod	steam	e	s	
# ::edge	turntable	mod	railway	t	r	
# ::edge	engine	location	turntable	e	t	
# ::edge	engine	prep-with	house	e	h	
# ::edge	house	location	background	h	b	
(e / engine
    :location (t / turntable
        :mod (r / railway))
    :mod (s / steam)
    :prep-with (h / house
        :location (b / background)))
