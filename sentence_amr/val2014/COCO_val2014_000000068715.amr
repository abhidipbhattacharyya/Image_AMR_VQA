# ::tok A street sign with the name of a street on it , and next to it is a post with various names up and down the post .
# ::node	s2	street	1-2
# ::node	s	sign	2-3
# ::node	n	name	5-6
# ::node	s3	street	8-9
# ::node	a	and	12-13
# ::node	n3	next-to	13-14
# ::node	p	post	18-19
# ::node	h	have-03	19-20
# ::node	v	various	20-21
# ::node	n2	name	21-22
# ::node	u	up-03	22-23
# ::node	a2	and	23-24
# ::node	d	down	24-25
# ::root	a	and
# ::edge	sign	mod	street	s	s2	
# ::edge	sign	poss	name	s	n	
# ::edge	name	mod	street	n	s3	
# ::edge	and	op1	sign	a	s	
# ::edge	next-to	op1	sign	n3	s	
# ::edge	post	location	next-to	p	n3	
# ::edge	and	op2	post	a	p	
# ::edge	post	ARG0-of	have-03	p	h	
# ::edge	name	mod	various	n2	v	
# ::edge	have-03	ARG1	name	h	n2	
# ::edge	up-03	ARG1	post	u	p	
# ::edge	and	op1	up-03	a2	u	
# ::edge	post	location	and	p	a2	
# ::edge	and	op2	down	a2	d	
# ::edge	down	op1	post	d	p	
(a / and
    :op1 (s / sign
        :mod (s2 / street)
        :poss (n / name
            :mod (s3 / street)))
    :op2 (p / post
        :ARG0-of (h / have-03
            :ARG1 (n2 / name
                :mod (v / various)))
        :location (n3 / next-to
            :op1 s)
        :location (a2 / and
            :op1 (u / up-03
                :ARG1 p)
            :op2 (d / down
                :op1 p))))
