        :op1 (s / set
            :quant (s3 / several)
            :consist-of (t / track
                :mod t3))
        :op2 (c / car
            :ARG1-of (e / extend-01
                :ARG2 (t2 / track
                    :consist-of (s2 / set
                        :quant 1))
                :direction (d / down))
            :mod t3)))
