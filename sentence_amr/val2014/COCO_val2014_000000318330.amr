# ::tok a small clock tower on a city street with people standing around it
# ::node	s	small	1-2
# ::node	c2	clock	2-3
# ::node	t	tower	3-4
# ::node	c	city	6-7
# ::node	s3	street	7-8
# ::node	p	person	9-10
# ::node	s2	stand-01	10-11
# ::node	a	around	11-12
# ::root	t	tower
# ::edge	tower	mod	clock	t	c2	
# ::edge	tower	mod	small	t	s	
# ::edge	street	mod	city	s3	c	
# ::edge	tower	location	street	t	s3	
# ::edge	stand-01	ARG1	person	s2	p	
# ::edge	tower	location-of	stand-01	t	s2	
# ::edge	stand-01	ARG2	around	s2	a	
# ::edge	around	op1	tower	a	t	
(t / tower
    :location (s3 / street
        :mod (c / city))
    :location-of (s2 / stand-01
        :ARG1 (p / person)
        :ARG2 (a / around
            :op1 t))
    :mod (c2 / clock)
    :mod (s / small))
