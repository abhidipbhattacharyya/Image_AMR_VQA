# ::tok a number of people with luggage bags outside of a building
# ::node	n	number	1-2
# ::node	p	person	3-4
# ::node	l	luggage	5-6
# ::node	b	bag	6-7
# ::node	o	outside	7-8
# ::node	b2	building	10-11
# ::root	p	person
# ::edge	person	quant	number	p	n	
# ::edge	bag	consist-of	luggage	b	l	
# ::edge	person	poss-of	bag	p	b	
# ::edge	person	location	outside	p	o	
# ::edge	outside	op1	building	o	b2	
(p / person
    :quant (n / number)
    :location (o / outside
        :op1 (b2 / building))
    :poss-of (b / bag
        :consist-of (l / luggage)))
