# ::tok A man with a bicycle with tanks for holding a liquid mounted on it .
# ::node	m	man	1-2
# ::node	b	bicycle	4-5
# ::node	t	tank	6-7
# ::node	h	hold-01	8-9
# ::node	l	liquid	10-11
# ::node	m2	mount-03	11-12
# ::root	m	man
# ::edge	man	poss-of	bicycle	m	b	
# ::edge	man	part	tank	m	t	
# ::edge	tank	ARG0-of	hold-01	t	h	
# ::edge	hold-01	ARG1	liquid	h	l	
# ::edge	liquid	ARG1-of	mount-03	l	m2	
# ::edge	mount-03	location	bicycle	m2	b	
(m / man
    :part (t / tank
        :ARG0-of (h / hold-01
            :ARG1 (l / liquid
                :ARG1-of (m2 / mount-03
                    :location (b / bicycle)))))
    :poss-of b)
