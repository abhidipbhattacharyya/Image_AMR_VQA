# ::tok A clock tower in the distance by a set of buildings at dusk .
# ::node	c	clock	1-2
# ::node	t	tower	2-3
# ::node	d2	distance	5-6
# ::node	s	set	8-9
# ::node	b	building	10-11
# ::node	d	date-entity	12-13
# ::node	d3	dusk	12-13
# ::root	t	tower
# ::edge	tower	mod	clock	t	c	
# ::edge	tower	location	distance	t	d2	
# ::edge	tower	prep-by	set	t	s	
# ::edge	set	consist-of	building	s	b	
# ::edge	tower	time	date-entity	t	d	
# ::edge	date-entity	dayperiod	dusk	d	d3	
(t / tower
    :location (d2 / distance)
    :mod (c / clock)
    :prep-by (s / set
        :consist-of (b / building))
    :time (d / date-entity
        :dayperiod (d3 / dusk)))
