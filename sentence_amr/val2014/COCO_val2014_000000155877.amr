# ::tok an adult sheep and baby sheep next to a tree in a grass field
# ::node	a	adult	1-2
# ::node	s	sheep	2-3
# ::node	a2	and	3-4
# ::node	b	baby	4-5
# ::node	s2	sheep	5-6
# ::node	n	next-to	6-7
# ::node	t	tree	9-10
# ::node	g	grass	12-13
# ::node	f	field	13-14
# ::root	a2	and
# ::edge	sheep	mod	adult	s	a	
# ::edge	and	op1	sheep	a2	s	
# ::edge	sheep	mod	baby	s2	b	
# ::edge	and	op2	sheep	a2	s2	
# ::edge	and	location	next-to	a2	n	
# ::edge	next-to	op1	tree	n	t	
# ::edge	field	mod	grass	f	g	
# ::edge	tree	location	field	t	f	
(a2 / and
    :location (n / next-to
        :op1 (t / tree
            :location (f / field
                :mod (g / grass))))
    :op1 (s / sheep
        :mod (a / adult))
    :op2 (s2 / sheep
        :mod (b / baby)))
