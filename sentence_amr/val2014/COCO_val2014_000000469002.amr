# ::tok A box with six divisions , each with its own variety of donut .
# ::node	b	box	1-2
# ::node	0	6	3-4
# ::node	d	division	4-5
# ::node	e	each	6-7
# ::node	h	have-03	7-8
# ::node	v	variety	10-11
# ::node	d2	donut	12-13
# ::root	b	box
# ::edge	division	quant	6	d	0	
# ::edge	box	part	division	b	d	
# ::edge	division	mod	each	d	e	
# ::edge	division	ARG0-of	have-03	d	h	
# ::edge	have-03	ARG1	variety	h	v	
# ::edge	variety	poss	division	v	d	
# ::edge	variety	mod	donut	v	d2	
(b / box
    :part (d / division
        :quant 6
        :ARG0-of (h / have-03
            :ARG1 (v / variety
                :mod (d2 / donut)
                :poss d))
        :mod (e / each)))
