# ::tok Two toasts in a plate with a knife and a fork .
# ::node	0	2	0-1
# ::node	t	toast-01	1-2
# ::node	p	plate	4-5
# ::node	k	knife	7-8
# ::node	a	and	8-9
# ::node	f	fork	10-11
# ::root	t	toast-01
# ::edge	toast-01	quant	2	t	0	
# ::edge	toast-01	location	plate	t	p	
# ::edge	and	op1	knife	a	k	
# ::edge	toast-01	instrument	and	t	a	
# ::edge	and	op2	fork	a	f	
(t / toast-01
    :quant 2
    :instrument (a / and
        :op1 (k / knife)
        :op2 (f / fork))
    :location (p / plate))


