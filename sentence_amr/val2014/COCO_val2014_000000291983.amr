# ::tok A large clock tower with two clocks mounted to it 's sides .
# ::node	l	large	1-2
# ::node	c	clock	2-3
# ::node	t	tower	3-4
# ::node	0	2	5-6
# ::node	w	watch	6-7
# ::node	m	mount-03	7-8
# ::node	s	side	11-12
# ::root	t	tower
# ::edge	tower	mod	clock	t	c	
# ::edge	tower	mod	large	t	l	
# ::edge	watch	quant	2	w	0	
# ::edge	mount-03	ARG1	watch	m	w	
# ::edge	tower	location-of	mount-03	t	m	
# ::edge	mount-03	ARG2	side	m	s	
# ::edge	tower	part	side	t	s	
(t / tower
    :location-of (m / mount-03
        :ARG1 (w / watch
            :quant 2)
        :ARG2 (s / side))
    :mod (c / clock)
    :mod (l / large)
    :part s)
