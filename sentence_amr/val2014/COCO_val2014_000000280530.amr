(t2 / tower
    :location (c / city
        :name (n2 / name
            :op1 "London")
        :location (c2 / country
            :name (n3 / name
                :op1 "England")))
    :mod (t / team
        :name (n / name
            :op1 "Big"
            :op2 "Been"
            :op3 "Parliament"))
    :time (d / day
        :mod (o / overcast)))
