# ::node	0	34th	11-12
# ::root	s	sign
# ::edge	sign	mod	street	s	s2	
# ::edge	sign	quant	multiple	s	m	
# ::edge	sign	location	corner	s	c	
# ::edge	name	op1	Park	n	3	
# ::edge	road	name	name	r	n	
# ::edge	name	op2	Ave	n	1	
# ::edge	and	op2	road	a	r2	
# ::edge	and	op1	road	a	r	
# ::edge	corner	op1	and	c	a	
# ::edge	road	name	name	r2	n2	
# ::edge	name	op1	E	n2	2	
# ::edge	name	op2	34th	n2	0	
(s / sign
    :quant (m / multiple)
    :location (c / corner
        :op1 (a / and
            :op1 (r / road
                :name (n / name
                    :op1 "Park"
                    :op2 "Ave"))
            :op2 (r2 / road
                :name (n2 / name
                    :op1 "E"
                    :op2 "34th"))))
    :mod (s2 / street))


