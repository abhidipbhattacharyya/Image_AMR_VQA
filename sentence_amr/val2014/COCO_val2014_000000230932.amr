# ::tok A set of three dummy heads with hats on top of their heads .
# ::node	s	set	1-2
# ::node	0	3	3-4
# ::node	d	dummy	4-5
# ::node	h2	head	5-6
# ::node	h	hat	7-8
# ::node	t	top	9-10
# ::root	s	set
# ::edge	head	mod	dummy	h2	d	
# ::edge	head	quant	3	h2	0	
# ::edge	set	consist-of	head	s	h2	
# ::edge	head	part	hat	h2	h	
# ::edge	top	location-of	hat	t	h	
# ::edge	head	location	top	h2	t	
(s / set
    :consist-of (h2 / head
        :quant 3
        :location (t / top
            :location-of (h / hat))
        :mod (d / dummy)
        :part h))
