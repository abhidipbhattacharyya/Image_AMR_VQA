# ::tok A clock with the numbers replaced with different types of birds .
# ::node	c	clock	1-2
# ::node	n	number	4-5
# ::node	r	replace-01	5-6
# ::node	d	differ-02	7-8
# ::node	t	type	8-9
# ::node	b	bird	10-11
# ::root	c	clock
# ::edge	replace-01	ARG1	number	r	n	
# ::edge	clock	prep-with	replace-01	c	r	
# ::edge	type	ARG1-of	differ-02	t	d	
# ::edge	bird	mod	type	b	t	
# ::edge	replace-01	ARG2	bird	r	b	
(c / clock
    :prep-with (r / replace-01
        :ARG1 (n / number)
        :ARG2 (b / bird
            :mod (t / type
                :ARG1-of (d / differ-02)))))
