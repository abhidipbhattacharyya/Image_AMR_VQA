# ::tok An umbrella table with chairs surrounding it .
# ::node	u	umbrella	1-2
# ::node	t	table	2-3
# ::node	c	chair	4-5
# ::node	s	surround-01	5-6
# ::root	t	table
# ::edge	table	mod	umbrella	t	u	
# ::edge	surround-01	ARG2	chair	s	c	
# ::edge	table	ARG1-of	surround-01	t	s	
(t / table
    :ARG1-of (s / surround-01
        :ARG2 (c / chair))
    :mod (u / umbrella))


