# ::tok two men playing frisbee in the water .
# ::node	0	2	0-1
# ::node	m	man	1-2
# ::node	p	play-01	2-3
# ::node	f	frisbee	3-4
# ::node	w	water	6-7
# ::root	p	play-01
# ::edge	man	quant	2	m	0	
# ::edge	play-01	ARG0	man	p	m	
# ::edge	play-01	ARG1	frisbee	p	f	
# ::edge	play-01	location	water	p	w	
(p / play-01
    :ARG0 (m / man
        :quant 2)
    :ARG1 (f / frisbee)
    :location (w / water))


# ::tok One man walking away
