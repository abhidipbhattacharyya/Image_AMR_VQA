(s / scene
    :mod (h / hydrant
        :location (s2 / slope
            :consist-of (g / grass)
            :location (i / in-front-of
                :op1 (b / building)))
        :mod (f / fire))
    :time (d / date-entity
        :dayperiod (n / night)))
