# ::node	s	scene	2-3
# ::node	f	fire	5-6
# ::node	h	hydrant	6-7
# ::node	g	grass	9-10
# ::node	s2	slope	10-11
# ::node	i	in-front-of	12-13
# ::node	b	building	15-16
# ::root	s	scene
# ::edge	date-entity	dayperiod	night	d	n	
# ::edge	scene	time	date-entity	s	d	
# ::edge	hydrant	mod	fire	h	f	
# ::edge	scene	mod	hydrant	s	h	
# ::edge	slope	consist-of	grass	s2	g	
# ::edge	hydrant	location	slope	h	s2	
