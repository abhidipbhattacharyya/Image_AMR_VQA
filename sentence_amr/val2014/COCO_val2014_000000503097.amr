# ::tok This is an interesting patterned clock on the side of a building .
# ::node	t	this	0-1
# ::node	i	interest-01	3-4
# ::node	p	pattern	4-5
# ::node	c	clock	5-6
# ::node	s	side	8-9
# ::node	b	building	11-12
# ::root	c	clock
# ::edge	pattern	ARG2-of	interest-01	p	i	
# ::edge	clock	mod	pattern	c	p	
# ::edge	clock	domain	this	c	t	
# ::edge	clock	location	side	c	s	
# ::edge	side	part-of	building	s	b	
(c / clock
    :domain (t / this)
    :location (s / side
        :part-of (b / building))
    :mod (p / pattern
        :ARG2-of (i / interest-01)))
