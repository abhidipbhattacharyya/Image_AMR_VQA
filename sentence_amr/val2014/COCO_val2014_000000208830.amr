# ::tok A very pointy building with a clock on each of it 's sides .
# ::node	v	very	1-2
# ::node	p	pointy	2-3
# ::node	b	building	3-4
# ::node	h	have-03	4-5
# ::node	c	clock	6-7
# ::node	e	each	8-9
# ::node	s	side	12-13
# ::root	b	building
# ::edge	pointy	degree	very	p	v	
# ::edge	building	mod	pointy	b	p	
# ::edge	building	ARG0-of	have-03	b	h	
# ::edge	have-03	ARG1	clock	h	c	
# ::edge	side	mod	each	s	e	
# ::edge	clock	location	side	c	s	
# ::edge	building	part	side	b	s	
(b / building
    :ARG0-of (h / have-03
        :ARG1 (c / clock
            :location (s / side
                :mod (e / each))))
    :mod (p / pointy
        :degree (v / very))
    :part s)
