(b / building
    :mod (t / type
        :consist-of (c2 / concrete))
    :part (a / and
        :op1 (w2 / window
            :quant (m / many))
        :op2 (c / clock
            :ARG0-of (h / have-03
                :ARG1 (n2 / number
                    :mod (c3 / country
                        :name (n / name
                            :op1 "Rome"))))
            :ARG1-of (a2 / attach-01
                :ARG2 (w / wall))))
    :part w)
