(s / sign
    :beneficiary (c2 / company
        :name (n / name
            :op1 "The"
            :op2 "Two"
            :op3 "Brewers"))
    :location (n3 / next-to
        :op1 (c / clock
            :mod (s2 / station
                :name (n2 / name
                    :op1 "Paddington"
                    :op2 "Station"))))
    :mod (s3 / style
        :mod (v / vintage)))
