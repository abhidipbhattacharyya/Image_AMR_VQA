# ::tok Tray of sandwiches that are half wrapped and half unwrapped
# ::node	t	tent	0-1
# ::node	s	sandwich	2-3
# ::node	h	half	5-6
# ::node	w	wrap-01	6-7
# ::node	w2	wrap-01	9-10
# ::node	0	-	9-10
# ::root	t	tent
# ::edge	tent	consist-of	sandwich	t	s	
# ::edge	wrap-01	mod	half	w	h	
# ::edge	sandwich	ARG1-of	wrap-01	s	w	
# ::edge	wrap-01	mod	half	w2	h	
# ::edge	sandwich	ARG1-of	wrap-01	s	w2	
# ::edge	wrap-01	polarity	-	w2	0	
(t / tent
    :consist-of (s / sandwich
        :ARG1-of (w / wrap-01
            :mod (h / half))
        :ARG1-of (w2 / wrap-01
            :mod h
            :polarity -)))
