(t2 / thing
    :ARG1-of (s / stick-01
        :ARG2 (l / limit-01
            :ARG1 (t3 / time
                :duration (t / temporal-quantity
                    :quant 3
                    :unit (h / hour)))))
    :rel (m / meter
        :purpose (p2 / park-01)
        :quant (p / pair)
        :mod (e / each)))
