# ::tok a clock on the side of a building saying it is 12:00
# ::node	c	clock	1-2
# ::node	s2	side	4-5
# ::node	b	building	7-8
# ::node	s	say-01	8-9
# ::node	d	date-entity	11-12
# ::node	0	12:00	11-12
# ::root	c	clock
# ::edge	clock	location	side	c	s2	
# ::edge	side	part-of	building	s2	b	
# ::edge	clock	ARG0-of	say-01	c	s	
# ::edge	say-01	ARG1	date-entity	s	d	
# ::edge	date-entity	time	12:00	d	0	
(c / clock
    :ARG0-of (s / say-01
        :ARG1 (d / date-entity
            :time "12:00"))
    :location (s2 / side
        :part-of (b / building)))
