# ::tok a number of animals in a field of grass with trees in the background
# ::node	n	number	1-2
# ::node	a	animal	3-4
# ::node	f	field	6-7
# ::node	g	grass	8-9
# ::node	t	tree	10-11
# ::node	b	background	13-14
# ::root	a	animal
# ::edge	animal	quant	number	a	n	
# ::edge	animal	location	field	a	f	
# ::edge	field	consist-of	grass	f	g	
# ::edge	field	location-of	tree	f	t	
# ::edge	field	location	background	f	b	
(a / animal
    :quant (n / number)
    :location (f / field
        :consist-of (g / grass)
        :location (b / background)
        :location-of (t / tree)))
