# ::node	b	banana	3-4
# ::node	b2	bowl	6-7
# ::node	f	face	8-9
# ::node	c	cause-01	11-12
# ::node	m	marker	13-14
# ::root	b	banana
# ::edge	banana	quant	bunch	b	b3	
# ::edge	banana	location	bowl	b	b2	
# ::edge	banana	part	face	b	f	
# ::edge	cause-01	ARG0	face	c	f	
# ::edge	banana	ARG1-of	cause-01	b	c	
# ::edge	face	source	marker	f	m	
(b / banana
    :quant (b3 / bunch)
    :ARG1-of (c / cause-01
        :ARG0 (f / face
            :source (m / marker)))
    :location (b2 / bowl)
    :part f)
