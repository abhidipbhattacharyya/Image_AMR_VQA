# ::tok a large tower with a clock built into the side of it
# ::node	l	large	1-2
# ::node	t	tower	2-3
# ::node	h	have-03	3-4
# ::node	c	clock	5-6
# ::node	b	build-01	6-7
# ::node	s	side	9-10
# ::root	t	tower
# ::edge	tower	mod	large	t	l	
# ::edge	tower	ARG0-of	have-03	t	h	
# ::edge	have-03	ARG1	clock	h	c	
# ::edge	clock	ARG1-of	build-01	c	b	
# ::edge	build-01	location	side	b	s	
# ::edge	tower	part	side	t	s	
(t / tower
    :ARG0-of (h / have-03
        :ARG1 (c / clock
            :ARG1-of (b / build-01
                :location (s / side))))
    :mod (l / large)
    :part s)
