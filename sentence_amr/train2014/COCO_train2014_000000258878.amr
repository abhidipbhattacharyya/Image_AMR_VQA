# ::tok An electric train amid all its tracks and wires in a city
# ::node	e	electric	1-2
# ::node	t2	train	2-3
# ::node	a	all	4-5
# ::node	t	track	6-7
# ::node	a2	and	7-8
# ::node	w	wire	8-9
# ::node	c	city	11-12
# ::root	t2	train
# ::edge	train	mod	electric	t2	e	
# ::edge	track	poss	train	t	t2	
# ::edge	and	op1	track	a2	t	
# ::edge	and	mod	all	a2	a	
# ::edge	train	location	and	t2	a2	
# ::edge	and	op2	wire	a2	w	
# ::edge	wire	poss	train	w	t2	
# ::edge	train	location	city	t2	c	
(t2 / train
    :location (a2 / and
        :mod (a / all)
        :op1 (t / track
            :poss t2)
        :op2 (w / wire
            :poss t2))
    :location (c / city)
    :mod (e / electric))
