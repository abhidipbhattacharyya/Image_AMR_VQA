# ::tok A view of a clock tower from the bottom of the tower .
# ::node	v	view-01	1-2
# ::node	c	clock	4-5
# ::node	t	tower	5-6
# ::node	b	bottom	8-9
# ::root	v	view-01
# ::edge	tower	mod	clock	t	c	
# ::edge	view-01	ARG1	tower	v	t	
# ::edge	tower	part	bottom	t	b	
# ::edge	view-01	source	bottom	v	b	
(v / view-01
    :ARG1 (t / tower
        :mod (c / clock)
        :part (b / bottom))
    :source b)


# ::tok Its very elegant in nature .
# ::node	i	it	0-1
# ::node	v	very	1-2
# ::node	e	elegant	2-3
# ::node	n	nature	4-5
# ::root	e	elegant
# ::edge	elegant	degree	very	e	v	
# ::edge	elegant	domain	it	e	i	
# ::edge	elegant	mod	nature	e	n	
(e / elegant
    :degree (v / very)
    :domain (i / it)
    :mod (n / nature))
