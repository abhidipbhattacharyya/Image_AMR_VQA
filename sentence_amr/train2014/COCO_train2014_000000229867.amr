# ::tok an image of a stop sign that is posted in the three way zone
# ::node	i	image	1-2
# ::node	s2	stop-01	4-5
# ::node	s	sign	5-6
# ::node	p	post-01	8-9
# ::node	0	3	11-12
# ::node	w	way	12-13
# ::node	z	zone	13-14
# ::root	i	image
# ::edge	sign	topic	stop-01	s	s2	
# ::edge	image	mod	sign	i	s	
# ::edge	sign	ARG1-of	post-01	s	p	
# ::edge	way	quant	3	w	0	
# ::edge	zone	mod	way	z	w	
# ::edge	post-01	ARG2	zone	p	z	
(i / image
    :mod (s / sign
        :ARG1-of (p / post-01
            :ARG2 (z / zone
                :mod (w / way
                    :quant 3)))
        :topic (s2 / stop-01)))
