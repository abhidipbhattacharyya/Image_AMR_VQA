# ::tok A large fancy clock on a building showing the time of 12:55 pm .
# ::node	l	large	1-2
# ::node	f	fancy	2-3
# ::node	c	clock	3-4
# ::node	b	building	6-7
# ::node	s	show-01	7-8
# ::node	t	time	9-10
# ::node	d	date-entity	11-12
# ::node	0	12	11-12
# ::node	1	2005	11-12
# ::root	c	clock
# ::edge	clock	mod	fancy	c	f	
# ::edge	clock	mod	large	c	l	
# ::edge	clock	location	building	c	b	
# ::edge	clock	ARG0-of	show-01	c	s	
# ::edge	show-01	ARG1	time	s	t	
# ::edge	time	time	date-entity	t	d	
# ::edge	date-entity	time	12	d	0	
# ::edge	date-entity	year	2005	d	1	
(c / clock
    :ARG0-of (s / show-01
        :ARG1 (t / time
            :time (d / date-entity
                :time 12
                :year 2005)))
    :location (b / building)
    :mod (f / fancy)
    :mod (l / large))
