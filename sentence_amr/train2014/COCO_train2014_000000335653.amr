# ::tok An old analog clock at a train station with the hands missing
# ::node	o	old	1-2
# ::node	a	analog	2-3
# ::node	c	clock	3-4
# ::node	t	train	6-7
# ::node	s	station	7-8
# ::node	h	hand	10-11
# ::node	m	miss-01	11-12
# ::root	c	clock
# ::edge	clock	mod	analog	c	a	
# ::edge	clock	mod	old	c	o	
# ::edge	station	mod	train	s	t	
# ::edge	clock	location	station	c	s	
# ::edge	miss-01	ARG1	hand	m	h	
# ::edge	clock	ARG0-of	miss-01	c	m	
(c / clock
    :ARG0-of (m / miss-01
        :ARG1 (h / hand))
    :location (s / station
        :mod (t / train))
    :mod (a / analog)
    :mod (o / old))
