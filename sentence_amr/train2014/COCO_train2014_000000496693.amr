# ::tok A group of men who are on the rear part of a passenger train .
# ::node	g	group	1-2
# ::node	m	man	3-4
# ::node	r	rear	8-9
# ::node	p	part	9-10
# ::node	p2	passenger	12-13
# ::node	t	train	13-14
# ::root	g	group
# ::edge	group	consist-of	man	g	m	
# ::edge	part	mod	rear	p	r	
# ::edge	group	location	part	g	p	
# ::edge	train	mod	passenger	t	p2	
