# ::tok A number of people in the snow walking , with skis on or a snowboard on .
# ::node	n	number	1-2
# ::node	p	person	3-4
# ::node	s3	snow	6-7
# ::node	w	walk-01	7-8
# ::node	s	ski-01	10-11
# ::node	o	or	12-13
# ::node	s2	ski-01	14-15
# ::root	p	person
# ::edge	person	quant	number	p	n	
# ::edge	person	location	snow	p	s3	
# ::edge	person	ARG0-of	walk-01	p	w	
# ::edge	ski-01	ARG0	person	s	p	
# ::edge	or	op1	ski-01	o	s	
# ::edge	person	instrument	or	p	o	
# ::edge	or	op2	ski-01	o	s2	
# ::edge	ski-01	ARG0	person	s2	p	
(p / person
    :quant (n / number)
    :ARG0-of (w / walk-01)
    :instrument (o / or
        :op1 (s / ski-01
            :ARG0 p)
        :op2 (s2 / ski-01
            :ARG0 p))
    :location (s3 / snow))
