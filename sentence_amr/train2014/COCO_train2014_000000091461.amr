# ::tok A tall building with a point on the top of it with a clock on it .
# ::node	t	tall	1-2
# ::node	b	building	2-3
# ::node	p	point	5-6
# ::node	t2	top	8-9
# ::node	c	clock	13-14
# ::root	b	building
# ::edge	building	mod	tall	b	t	
# ::edge	building	part	point	b	p	
# ::edge	point	location	top	p	t2	
# ::edge	building	part	top	b	t2	
# ::edge	point	location-of	clock	p	c	
(b / building
    :mod (t / tall)
    :part (p / point
        :location (t2 / top)
        :location-of (c / clock))
    :part t2)
