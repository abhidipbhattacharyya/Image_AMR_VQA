# ::tok A train station with people standing and sitting while waiting for their trains .
# ::node	t	train	1-2
# ::node	s3	station	2-3
# ::node	p	person	4-5
# ::node	s2	stand-01	5-6
# ::node	a	and	6-7
# ::node	s	sit-01	7-8
# ::node	w	wait-01	9-10
# ::node	t2	train	12-13
# ::root	s3	station
# ::edge	station	mod	train	s3	t	
# ::edge	stand-01	ARG1	person	s2	p	
# ::edge	and	op1	stand-01	a	s2	
# ::edge	station	location-of	and	s3	a	
# ::edge	and	op2	sit-01	a	s	
# ::edge	sit-01	ARG1	person	s	p	
# ::edge	and	time	wait-01	a	w	
# ::edge	wait-01	ARG1	person	w	p	
# ::edge	wait-01	ARG2	train	w	t2	
# ::edge	train	poss	person	t2	p	
(s3 / station
    :location-of (a / and
        :op1 (s2 / stand-01
            :ARG1 (p / person))
        :op2 (s / sit-01
            :ARG1 p)
        :time (w / wait-01
            :ARG1 p
            :ARG2 (t2 / train
                :poss p)))
    :mod (t / train))
