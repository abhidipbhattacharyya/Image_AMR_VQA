# ::tok A random building called Essex Beds which has been in business for 25 years .
# ::node	r	random	1-2
# ::node	b	building	2-3
# ::node	n	name	2-3
# ::node	2	Essex	4-5
# ::node	1	Beds	5-6
# ::node	b2	business	10-11
# ::node	t	temporal-quantity	12-13
# ::node	0	25	12-13
# ::node	y	year	13-14
# ::root	b	building
# ::edge	building	mod	random	b	r	
# ::edge	building	name	name	b	n	
# ::edge	name	op1	Essex	n	2	
# ::edge	name	op2	Beds	n	1	
# ::edge	building	mod	business	b	b2	
# ::edge	business	duration	temporal-quantity	b2	t	
