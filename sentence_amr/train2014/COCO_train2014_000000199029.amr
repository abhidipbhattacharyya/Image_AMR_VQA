(c / car
    :quant (n / number)
    :ARG0-of (j / jam-01
        :ARG1 (t / traffic))
    :ARG0-of (h / have-03
        :ARG1 (a / and
            :location (r2 / roadside)
            :op1 (l / light
                :mod (t2 / traffic))
            :op2 (s / sign
                :mod (r / road)))))
