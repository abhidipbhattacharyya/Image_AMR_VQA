# ::node	t	traffic	6-7
# ::node	j	jam-01	7-8
# ::node	h	have-03	8-9
# ::node	t2	traffic	9-10
# ::node	l	light	10-11
# ::node	a	and	11-12
# ::node	r	road	12-13
# ::node	s	sign	13-14
# ::node	r2	roadside	16-17
