# ::tok A table with stacks of books on it with a mouse , keyboard and a monitor on top of them .
# ::node	t	table	1-2
# ::node	s	stack-01	3-4
# ::node	b	book	5-6
# ::node	m2	mouse	10-11
# ::node	k	keyboard	12-13
# ::node	a	and	13-14
# ::node	m	monitor	15-16
# ::node	t2	top	17-18
# ::root	t	table
# ::edge	book	ARG1-of	stack-01	b	s	
# ::edge	table	location-of	book	t	b	
# ::edge	and	op2	keyboard	a	k	
# ::edge	and	op1	mouse	a	m2	
# ::edge	table	instrument	and	t	a	
# ::edge	and	op3	monitor	a	m	
# ::edge	and	location	top	a	t2	
# ::edge	top	op1	book	t2	b	
(t / table
    :instrument (a / and
        :location (t2 / top
            :op1 (b / book
                :ARG1-of (s / stack-01)))
        :op1 (m2 / mouse)
        :op2 (k / keyboard)
        :op3 (m / monitor))
    :location-of b)
