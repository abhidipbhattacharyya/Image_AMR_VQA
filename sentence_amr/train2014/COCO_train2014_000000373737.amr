# ::tok An architectural clock in a historical building in Prague .
# ::node	a	archeology	1-2
# ::node	c2	clock	2-3
# ::node	h	historical	5-6
# ::node	b	building	6-7
# ::node	c	city	8-9
# ::node	n	name	8-9
# ::node	0	Prague	8-9
# ::root	c2	clock
# ::edge	clock	mod	archeology	c2	a	
# ::edge	building	mod	historical	b	h	
# ::edge	clock	location	building	c2	b	
# ::edge	building	location	city	b	c	
# ::edge	city	name	name	c	n	
# ::edge	name	op1	Prague	n	0	
(c2 / clock
    :location (b / building
        :location (c / city
            :name (n / name
                :op1 "Prague"))
        :mod (h / historical))
    :mod (a / archeology))
