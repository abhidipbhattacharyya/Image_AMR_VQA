# ::tok A large clock tower with a clock on each of it 's four sides .
# ::node	l	large	1-2
# ::node	c	clock	2-3
# ::node	t	tower	3-4
# ::node	h	have-03	4-5
# ::node	c2	clock	6-7
# ::node	e	each	8-9
# ::node	0	4	12-13
# ::node	s	side	13-14
# ::root	t	tower
# ::edge	tower	mod	clock	t	c	
# ::edge	tower	mod	large	t	l	
# ::edge	tower	ARG0-of	have-03	t	h	
# ::edge	have-03	ARG1	clock	h	c2	
# ::edge	side	quant	4	s	0	
# ::edge	side	mod	each	s	e	
# ::edge	clock	location	side	c2	s	
# ::edge	tower	part	side	t	s	
(t / tower
    :ARG0-of (h / have-03
        :ARG1 (c2 / clock
            :location (s / side
                :quant 4
                :mod (e / each))))
    :mod (c / clock)
    :mod (l / large)
    :part s)
