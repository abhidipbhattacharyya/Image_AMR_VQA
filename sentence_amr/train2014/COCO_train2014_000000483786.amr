# ::tok A large clock tower with massive clocks on each of it 's sides .
# ::node	l	large	1-2
# ::node	c	clock	2-3
# ::node	t	tower	3-4
# ::node	h	have-03	4-5
# ::node	m	massive	5-6
# ::node	w	watch	6-7
# ::node	e	each	8-9
# ::node	s	side	12-13
# ::root	t	tower
# ::edge	tower	mod	clock	t	c	
# ::edge	tower	mod	large	t	l	
# ::edge	tower	ARG0-of	have-03	t	h	
# ::edge	watch	mod	massive	w	m	
# ::edge	have-03	ARG1	watch	h	w	
# ::edge	side	mod	each	s	e	
# ::edge	watch	location	side	w	s	
# ::edge	tower	part	side	t	s	
(t / tower
    :ARG0-of (h / have-03
        :ARG1 (w / watch
            :location (s / side
                :mod (e / each))
            :mod (m / massive)))
    :mod (c / clock)
    :mod (l / large)
    :part s)
