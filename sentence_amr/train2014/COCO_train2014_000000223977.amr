# ::tok A plate of food with some eggs , bacon , ham , and bread on it .
# ::node	p	plate	1-2
# ::node	f	food	3-4
# ::node	s	some	5-6
# ::node	e	egg	6-7
# ::node	b	bacon	8-9
# ::node	h	ham	10-11
# ::node	a	and	12-13
# ::node	b2	bread	13-14
# ::root	p	plate
# ::edge	plate	consist-of	food	p	f	
# ::edge	egg	mod	some	e	s	
# ::edge	bacon	mod	some	b	s	
# ::edge	and	op3	ham	a	h	
# ::edge	and	op2	bacon	a	b	
# ::edge	and	op1	egg	a	e	
# ::edge	plate	consist-of	and	p	a	
# ::edge	and	op4	bread	a	b2	
(p / plate
    :consist-of (f / food)
    :consist-of (a / and
        :op1 (e / egg
            :mod (s / some))
        :op2 (b / bacon
            :mod s)
        :op3 (h / ham)
        :op4 (b2 / bread)))
