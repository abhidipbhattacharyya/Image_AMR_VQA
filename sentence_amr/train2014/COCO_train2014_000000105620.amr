# ::tok A bowl of banana pudding with banana slices and sauce .
# ::node	b2	bowl	1-2
# ::node	b	banana	3-4
# ::node	p	pudding	4-5
# ::node	s2	slice	7-8
# ::node	a	and	8-9
# ::node	s	sauce	9-10
# ::root	p	pudding
# ::edge	pudding	mod	banana	p	b	
# ::edge	pudding	quant	bowl	p	b2	
# ::edge	slice	mod	banana	s2	b	
# ::edge	and	op1	slice	a	s2	
# ::edge	pudding	accompanier	and	p	a	
# ::edge	and	op2	sauce	a	s	
# ::edge	sauce	mod	banana	s	b	
(p / pudding
    :quant (b2 / bowl)
    :accompanier (a / and
        :op1 (s2 / slice
            :mod (b / banana))
        :op2 (s / sauce
            :mod b))
    :mod b)
