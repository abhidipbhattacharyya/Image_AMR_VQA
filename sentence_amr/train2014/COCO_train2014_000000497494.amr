# ::tok Two men sitting in bed with their dog .
# ::node	0	2	0-1
# ::node	m	man	1-2
# ::node	s	sit-01	2-3
# ::node	b	bed	4-5
# ::node	d	dog	7-8
# ::root	s	sit-01
# ::edge	man	quant	2	m	0	
# ::edge	sit-01	ARG1	man	s	m	
# ::edge	sit-01	ARG2	bed	s	b	
# ::edge	man	accompanier	dog	m	d	
# ::edge	dog	poss	man	d	m	
(s / sit-01
    :ARG1 (m / man
        :quant 2
        :accompanier (d / dog
            :poss m))
    :ARG2 (b / bed))


