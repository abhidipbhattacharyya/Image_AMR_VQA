# ::tok an image of a clock that is next to a group of books
# ::node	i	image	1-2
# ::node	c	clock	4-5
# ::node	n	next-to	7-8
# ::node	g	group	10-11
# ::node	b	book	12-13
# ::root	i	image
# ::edge	image	mod	clock	i	c	
# ::edge	clock	location	next-to	c	n	
# ::edge	next-to	op1	group	n	g	
# ::edge	group	consist-of	book	g	b	
(i / image
    :mod (c / clock
        :location (n / next-to
            :op1 (g / group
                :consist-of (b / book)))))
