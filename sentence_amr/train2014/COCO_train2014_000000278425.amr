# ::tok A boy looks at his phone screen with his fingers on the buttons .
# ::node	b	boy	1-2
# ::node	l	look-01	2-3
# ::node	p	phone	5-6
# ::node	s	screen	6-7
# ::node	f	finger	9-10
# ::node	b2	button	12-13
# ::root	l	look-01
# ::edge	look-01	ARG0	boy	l	b	
# ::edge	phone	poss	boy	p	b	
# ::edge	screen	part-of	phone	s	p	
# ::edge	look-01	ARG1	screen	l	s	
# ::edge	look-01	instrument	finger	l	f	
# ::edge	boy	part	finger	b	f	
# ::edge	finger	location	button	f	b2	
(l / look-01
    :ARG0 (b / boy
        :part (f / finger
            :location (b2 / button)))
    :ARG1 (s / screen
        :part-of (p / phone
            :poss b))
    :instrument f)
