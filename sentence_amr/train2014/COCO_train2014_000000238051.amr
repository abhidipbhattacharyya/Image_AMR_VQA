# ::node	d2	door	5-6
# ::node	o	open-01	7-8
# ::node	a	and	8-9
# ::node	e2	empty-02	11-12
# ::root	a	and
# ::edge	door	mod	electronic	d2	e	
# ::edge	door	part-of	door	d	d2	
# ::edge	open-01	ARG1	door	o	d	
# ::edge	and	op1	open-01	a	o	
# ::edge	and	op2	empty-02	a	e2	
# ::edge	empty-02	ARG1	door	e2	d	
