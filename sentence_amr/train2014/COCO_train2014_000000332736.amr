# ::tok A grandfather style clock against a wall and another pendulum clock hanging on the wall next to it .
# ::node	g	grandfather	1-2
# ::node	s	style	2-3
# ::node	c	clock	3-4
# ::node	a	against	4-5
# ::node	w	wall	6-7
# ::node	a2	and	7-8
# ::node	a3	another	8-9
# ::node	p	pendulum	9-10
# ::node	c2	clock	10-11
# ::node	h	hang-01	11-12
# ::node	w2	wall	14-15
# ::node	n	next-to	15-16
# ::root	a2	and
# ::edge	style	mod	grandfather	s	g	
# ::edge	clock	mod	style	c	s	
# ::edge	clock	location	against	c	a	
# ::edge	against	op1	wall	a	w	
# ::edge	and	op1	clock	a2	c	
# ::edge	clock	mod	pendulum	c2	p	
# ::edge	clock	mod	another	c2	a3	
# ::edge	and	op2	clock	a2	c2	
# ::edge	clock	ARG1-of	hang-01	c2	h	
# ::edge	hang-01	location	wall	h	w2	
# ::edge	wall	location	next-to	w2	n	
# ::edge	next-to	op1	wall	n	w	
(a2 / and
    :op1 (c / clock
        :location (a / against
            :op1 (w / wall))
        :mod (s / style
            :mod (g / grandfather)))
    :op2 (c2 / clock
        :ARG1-of (h / hang-01
            :location (w2 / wall
                :location (n / next-to
                    :op1 w)))
        :mod (p / pendulum)
        :mod (a3 / another)))
