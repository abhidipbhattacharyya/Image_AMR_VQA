(p2 / plate
    :consist-of (a / and
        :op1 (b / bean)
        :op2 (b2 / broccoli)
        :op3 (o / or
            :mod (k / kind
                :mod (s2 / some))
            :op1 (s / sandwich)
            :op2 (p / pizza))))
