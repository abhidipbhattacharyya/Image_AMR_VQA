# ::node	c	computer	2-3
# ::node	d	desk	3-4
# ::node	l	lot	6-7
# ::node	w	wire	8-9
# ::node	u	underneath	9-10
# ::root	d	desk
# ::edge	desk	mod	computer	d	c	
# ::edge	desk	consist-of	metal	d	m	
# ::edge	wire	quant	lot	w	l	
