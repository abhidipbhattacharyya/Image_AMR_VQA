(a / and
    :op1 (n / nail
        :quant (p / pair)
        :location (n2 / next-to
            :op1 (b / bar
                :ARG1-of (u / use-01
                    :degree (p2 / part))
                :consist-of (b2 / butter))))
    :op2 (m / mitt
        :location (n3 / next-to
            :op1 n)
        :mod (o / oven)))
