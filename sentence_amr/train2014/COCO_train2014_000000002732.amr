# ::tok A number of locomotive train engines and cars on multiple set of train tracks .
# ::node	n	number	1-2
# ::node	l	locomotive	3-4
# ::node	t2	train	4-5
# ::node	e	engine	5-6
# ::node	a	and	6-7
# ::node	c	car	7-8
# ::node	m	multiple	9-10
# ::node	s	set	10-11
# ::node	t3	train	12-13
# ::node	t	track	13-14
# ::root	a	and
# ::edge	train	mod	locomotive	t2	l	
# ::edge	engine	mod	train	e	t2	
# ::edge	and	op1	engine	a	e	
# ::edge	and	quant	number	a	n	
# ::edge	and	op2	car	a	c	
# ::edge	car	mod	train	c	t2	
# ::edge	set	quant	multiple	s	m	
# ::edge	and	location	set	a	s	
# ::edge	track	mod	train	t	t3	
# ::edge	set	consist-of	track	s	t	
(a / and
    :quant (n / number)
    :location (s / set
        :quant (m / multiple)
        :consist-of (t / track
            :mod (t3 / train)))
    :op1 (e / engine
        :mod (t2 / train
            :mod (l / locomotive)))
    :op2 (c / car
        :mod t2))
