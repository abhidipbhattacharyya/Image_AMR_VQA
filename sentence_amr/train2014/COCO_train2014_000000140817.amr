# ::tok A building with a clock on the front and side of it while the sun goes down .
# ::node	b	building	1-2
# ::node	h	have-03	2-3
# ::node	c	clock	4-5
# ::node	f	front	7-8
# ::node	a	and	8-9
# ::node	s	side	9-10
# ::node	s2	sun	14-15
# ::node	g	go-01	15-16
# ::node	d	down-01	16-17
# ::root	b	building
# ::edge	building	ARG0-of	have-03	b	h	
# ::edge	have-03	ARG1	clock	h	c	
# ::edge	and	op1	front	a	f	
# ::edge	clock	location	and	c	a	
# ::edge	building	part	and	b	a	
# ::edge	and	op2	side	a	s	
# ::edge	go-01	ARG1	sun	g	s2	
# ::edge	have-03	time	go-01	h	g	
# ::edge	go-01	ARG4	down-01	g	d	
# ::edge	down-01	ARG1	sun	d	s2	
(b / building
    :ARG0-of (h / have-03
        :ARG1 (c / clock
            :location (a / and
                :op1 (f / front)
                :op2 (s / side)))
        :time (g / go-01
            :ARG1 (s2 / sun)
            :ARG4 (d / down-01
                :ARG1 s2)))
    :part a)
