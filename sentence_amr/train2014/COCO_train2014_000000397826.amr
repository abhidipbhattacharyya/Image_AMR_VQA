# ::tok Brick and stone clock tower with letters instead of numbers .
# ::node	b	brick	0-1
# ::node	a	and	1-2
# ::node	s	stone	2-3
# ::node	c	clock	3-4
# ::node	t	tower	4-5
# ::node	t2	tower	4-5
# ::node	l	letter	6-7
# ::node	i	instead-of-91	7-8
# ::node	n	number	9-10
# ::root	a	and
# ::edge	tower	mod	clock	t	c	
# ::edge	and	op1	tower	a	t	
# ::edge	tower	mod	brick	t	b	
# ::edge	tower	mod	clock	t2	c	
# ::edge	tower	mod	stone	t2	s	
# ::edge	and	op2	tower	a	t2	
# ::edge	and	prep-with	letter	a	l	
# ::edge	letter	ARG1-of	instead-of-91	l	i	
# ::edge	instead-of-91	ARG2	number	i	n	
(a / and
    :op1 (t / tower
        :mod (c / clock)
        :mod (b / brick))
    :op2 (t2 / tower
        :mod c
        :mod (s / stone))
    :prep-with (l / letter
        :ARG1-of (i / instead-of-91
            :ARG2 (n / number))))
