# ::tok A bunch of keyboards that are sitting next to a group of mouse 's .
# ::node	b	bunch	1-2
# ::node	k	keyboard	3-4
# ::node	s	sit-01	6-7
# ::node	n	next-to	7-8
# ::node	g	group	10-11
# ::node	m	mouse	12-13
# ::root	s	sit-01
# ::edge	keyboard	quant	bunch	k	b	
# ::edge	sit-01	ARG1	keyboard	s	k	
# ::edge	sit-01	ARG2	next-to	s	n	
# ::edge	next-to	op1	group	n	g	
# ::edge	group	consist-of	mouse	g	m	
(s / sit-01
    :ARG1 (k / keyboard
        :quant (b / bunch))
    :ARG2 (n / next-to
        :op1 (g / group
            :consist-of (m / mouse))))
