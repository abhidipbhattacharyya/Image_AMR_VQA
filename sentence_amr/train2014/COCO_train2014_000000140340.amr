# ::tok Inside a radio station for passengers there is a clock above their heads .
# ::node	i	inside	0-1
# ::node	r	radio-01	2-3
# ::node	s	station	3-4
# ::node	p	passenger	5-6
# ::node	c	clock	9-10
# ::node	a	above	10-11
# ::node	h	head	12-13
# ::root	c	clock
# ::edge	station	ARG0-of	radio-01	s	r	
# ::edge	inside	op1	station	i	s	
# ::edge	station	beneficiary	passenger	s	p	
# ::edge	clock	location	inside	c	i	
# ::edge	clock	location	above	c	a	
# ::edge	above	op1	head	a	h	
# ::edge	passenger	part	head	p	h	
(c / clock
    :location (i / inside
        :op1 (s / station
            :ARG0-of (r / radio-01)
            :beneficiary (p / passenger
                :part (h / head))))
    :location (a / above
        :op1 h))
