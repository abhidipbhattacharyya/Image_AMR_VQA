# ::tok The sign in front of a French bar which indicates the location of the bar .
# ::node	s	sign	1-2
# ::node	i	in-front-of	3-4
# ::node	c	country	6-7
# ::node	n	name	6-7
# ::node	0	France	6-7
# ::node	b	bar	7-8
# ::node	i2	indicate-01	9-10
# ::node	l	location	11-12
# ::root	s	sign
# ::edge	sign	location	in-front-of	s	i	
# ::edge	country	name	name	c	n	
# ::edge	name	op1	France	n	0	
# ::edge	bar	mod	country	b	c	
# ::edge	in-front-of	op1	bar	i	b	
# ::edge	sign	ARG0-of	indicate-01	s	i2	
# ::edge	indicate-01	ARG1	location	i2	l	
# ::edge	location	poss	bar	l	b	
(s / sign
    :ARG0-of (i2 / indicate-01
        :ARG1 (l / location
            :poss (b / bar
                :mod (c / country
                    :name (n / name
                        :op1 "France")))))
    :location (i / in-front-of
        :op1 b))
