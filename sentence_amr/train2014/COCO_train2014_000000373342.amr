# ::tok An old clock with a table and a chair on either side of it .
# ::node	o	old	1-2
# ::node	c2	clock	2-3
# ::node	h	have-03	3-4
# ::node	t	table	5-6
# ::node	a	and	6-7
# ::node	c	chair	8-9
# ::node	e	either	10-11
# ::node	s	side	11-12
# ::root	c2	clock
# ::edge	clock	mod	old	c2	o	
# ::edge	clock	ARG0-of	have-03	c2	h	
# ::edge	and	op1	table	a	t	
# ::edge	have-03	ARG1	and	h	a	
# ::edge	and	op2	chair	a	c	
# ::edge	side	mod	either	s	e	
# ::edge	and	location	side	a	s	
# ::edge	clock	part	side	c2	s	
(c2 / clock
    :ARG0-of (h / have-03
        :ARG1 (a / and
            :location (s / side
                :mod (e / either))
            :op1 (t / table)
            :op2 (c / chair)))
    :mod (o / old)
    :part s)
