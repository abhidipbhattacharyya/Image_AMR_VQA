# ::tok a diesel train next to the platform of a train station
# ::node	d	diesel	1-2
# ::node	t	train	2-3
# ::node	n	next-to	3-4
# ::node	p	platform	6-7
# ::node	t2	train	9-10
# ::node	s	station	10-11
# ::root	t	train
# ::edge	train	mod	diesel	t	d	
# ::edge	train	location	next-to	t	n	
# ::edge	next-to	op1	platform	n	p	
# ::edge	station	mod	train	s	t2	
# ::edge	platform	poss	station	p	s	
(t / train
    :location (n / next-to
        :op1 (p / platform
            :poss (s / station
                :mod (t2 / train))))
    :mod (d / diesel))
