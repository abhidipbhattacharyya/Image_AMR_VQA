# ::tok The last octave of piano keys on a keyboard with a sound board .
# ::node	l	last	1-2
# ::node	o	octave	2-3
# ::node	p	piano	4-5
# ::node	k	key	5-6
# ::node	k2	keyboard	8-9
# ::node	s	sound	11-12
# ::node	b	board	12-13
# ::root	o	octave
# ::edge	octave	mod	last	o	l	
# ::edge	key	mod	piano	k	p	
# ::edge	octave	poss	key	o	k	
# ::edge	key	location	keyboard	k	k2	
# ::edge	board	mod	sound	b	s	
# ::edge	keyboard	part	board	k2	b	
(o / octave
    :mod (l / last)
    :poss (k / key
        :location (k2 / keyboard
            :part (b / board
                :mod (s / sound)))
        :mod (p / piano)))
