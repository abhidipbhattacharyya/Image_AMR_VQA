# ::tok A clock attached to iron poles with the name underneath .
# ::node	c	clock	1-2
# ::node	a	attach-01	2-3
# ::node	i	iron	4-5
# ::node	p	pole	5-6
# ::node	n	name	8-9
# ::node	u	underneath	9-10
# ::root	c	clock
# ::edge	clock	ARG1-of	attach-01	c	a	
# ::edge	pole	consist-of	iron	p	i	
# ::edge	attach-01	ARG2	pole	a	p	
# ::edge	clock	prep-with	name	c	n	
# ::edge	pole	location	underneath	p	u	
(c / clock
    :ARG1-of (a / attach-01
        :ARG2 (p / pole
            :consist-of (i / iron)
            :location (u / underneath)))
    :prep-with (n / name))
