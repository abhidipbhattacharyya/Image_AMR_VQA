# ::tok a tower with a large clock built into the side of it
# ::node	t	tower	1-2
# ::node	h	have-03	2-3
# ::node	l	large	4-5
# ::node	c	clock	5-6
# ::node	b	build-01	6-7
# ::node	s	side	9-10
# ::root	t	tower
# ::edge	tower	ARG0-of	have-03	t	h	
# ::edge	clock	mod	large	c	l	
# ::edge	build-01	ARG1	clock	b	c	
# ::edge	have-03	ARG1	build-01	h	b	
# ::edge	build-01	location	side	b	s	
# ::edge	tower	part	side	t	s	
(t / tower
    :ARG0-of (h / have-03
        :ARG1 (b / build-01
            :ARG1 (c / clock
                :mod (l / large))
            :location (s / side)))
    :part s)
