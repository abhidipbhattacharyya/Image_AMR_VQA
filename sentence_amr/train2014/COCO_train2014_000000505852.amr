# ::tok A clock tower of a building with the moon rising near it
# ::node	c	clock	1-2
# ::node	t	tower	2-3
# ::node	b	building	5-6
# ::node	m	moon	8-9
# ::node	r	rise-01	9-10
# ::node	n	near-02	10-11
# ::root	t	tower
# ::edge	tower	mod	clock	t	c	
# ::edge	tower	part-of	building	t	b	
# ::edge	rise-01	ARG1	moon	r	m	
# ::edge	tower	location-of	rise-01	t	r	
# ::edge	rise-01	ARG1-of	near-02	r	n	
# ::edge	near-02	ARG2	tower	n	t	
(t / tower
    :location-of (r / rise-01
        :ARG1 (m / moon)
        :ARG1-of (n / near-02
            :ARG2 t))
    :mod (c / clock)
    :part-of (b / building))
