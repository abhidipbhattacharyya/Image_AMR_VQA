# ::tok A group of tables with flowers that are behind some bikes with flowers in their baskets .
# ::node	g	group	1-2
# ::node	t	table	3-4
# ::node	b2	behind	8-9
# ::node	s	some	9-10
# ::node	b3	bike-01	10-11
# ::node	f	flower	12-13
# ::node	b	basket	15-16
# ::root	g	group
# ::edge	group	consist-of	table	g	t	
# ::edge	table	location	behind	t	b2	
# ::edge	bike-01	quant	some	b3	s	
# ::edge	behind	op1	bike-01	b2	b3	
# ::edge	table	part	flower	t	f	
# ::edge	basket	poss	table	b	t	
# ::edge	group	location-of	basket	g	b	
(g / group
    :consist-of (t / table
        :location (b2 / behind
            :op1 (b3 / bike-01
                :quant (s / some)))
        :part (f / flower))
    :location-of (b / basket
        :poss t))
