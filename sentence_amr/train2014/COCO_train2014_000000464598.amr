(t / table
    :location (s / setting
        :quant 1
        :mod (p3 / place
            :mod (e / each)))
    :ord (m / multi-sentence
        :snt1 t)
    :rel (a / and
        :op1 (p / person
            :quant 2)
        :op2 (p2 / pizzeria
            :quant 2)))
