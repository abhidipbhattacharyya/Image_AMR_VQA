# ::node	c	clock	2-3
# ::node	s	some	4-5
# ::node	m	metal	5-6
# ::node	p	part	6-7
# ::node	t2	top	8-9
# ::node	t	table	11-12
# ::root	c	clock
# ::edge	clock	consist-of	wood	c	w	
# ::edge	part	consist-of	metal	p	m	
# ::edge	part	quant	some	p	s	
# ::edge	top	location-of	part	t2	p	
# ::edge	clock	part	top	c	t2	
# ::edge	top	part-of	table	t2	t	
(c / clock
    :consist-of (w / wood)
    :part (t2 / top
        :location-of (p / part
            :quant (s / some)
            :consist-of (m / metal))
        :part-of (t / table)))
