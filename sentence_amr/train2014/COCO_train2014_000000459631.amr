(s2 / stand-01
    :ARG1 (c / clock
        :ARG1-of (d3 / display-01
            :ARG2 (d2 / date-interval
                :op1 (t / temporal-quantity
                    :quant 10
                    :unit (m / minute))
                :op2 (d / date-entity
                    :time "14:00"))))
    :ARG2 (s / side
        :mod (i / interior)
        :part-of (w / window)))
