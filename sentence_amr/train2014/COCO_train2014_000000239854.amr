# ::tok A road sign that reads Main RD .
# ::node	r2	road	1-2
# ::node	s	sign	2-3
# ::node	r	read-01	4-5
# ::node	m	main	5-6
# ::node	r3	road	6-7
# ::node	n	name	6-7
# ::node	0	RD	6-7
# ::root	s	sign
# ::edge	sign	mod	road	s	r2	
# ::edge	sign	ARG1-of	read-01	s	r	
# ::edge	road	mod	main	r3	m	
# ::edge	read-01	ARG3	road	r	r3	
# ::edge	road	name	name	r3	n	
# ::edge	name	op1	RD	n	0	
(s / sign
    :ARG1-of (r / read-01
        :ARG3 (r3 / road
            :name (n / name
                :op1 "RD")
            :mod (m / main)))
    :mod (r2 / road))


