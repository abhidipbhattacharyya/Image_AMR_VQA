# ::tok Multiple colors and sizes of motorcycles are in a line .
# ::node	m2	multiple	0-1
# ::node	c	color	1-2
# ::node	a	and	2-3
# ::node	s	size	3-4
# ::node	m	motorcycle	5-6
# ::node	l	line	9-10
# ::root	l	line
# ::edge	color	quant	multiple	c	m2	
# ::edge	and	op1	color	a	c	
# ::edge	and	op2	size	a	s	
# ::edge	size	quant	multiple	s	m2	
# ::edge	size	poss	motorcycle	s	m	
# ::edge	color	mod	motorcycle	c	m	
# ::edge	line	domain	and	l	a	
(l / line
    :domain (a / and
        :op1 (c / color
            :quant (m2 / multiple)
            :mod (m / motorcycle))
        :op2 (s / size
            :quant m2
            :poss m)))
