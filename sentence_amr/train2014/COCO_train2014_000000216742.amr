# ::tok A line of double decker buses with a clock tower in the background .
# ::node	l	line	1-2
# ::node	d	deck	4-5
# ::node	b2	bus	5-6
# ::node	c	clock	8-9
# ::node	t	tower	9-10
# ::node	b	background	12-13
# ::root	l	line
# ::edge	bus	mod	deck	b2	d	
# ::edge	line	consist-of	bus	l	b2	
# ::edge	tower	mod	clock	t	c	
# ::edge	line	prep-with	tower	l	t	
# ::edge	tower	location	background	t	b	
(l / line
    :consist-of (b2 / bus
        :mod (d / deck))
    :prep-with (t / tower
        :location (b / background)
        :mod (c / clock)))
