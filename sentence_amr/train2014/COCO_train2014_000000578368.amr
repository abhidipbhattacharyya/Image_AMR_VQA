# ::tok A very tall tower next to a building with a clock on it 's side .
# ::node	v	very	1-2
# ::node	t	tall	2-3
# ::node	t2	tower	3-4
# ::node	n	next-to	4-5
# ::node	b	building	7-8
# ::node	h	have-03	8-9
# ::node	c	clock	10-11
# ::node	s	side	14-15
# ::root	t2	tower
# ::edge	tall	degree	very	t	v	
# ::edge	tower	mod	tall	t2	t	
# ::edge	tower	location	next-to	t2	n	
# ::edge	next-to	op1	building	n	b	
# ::edge	building	ARG0-of	have-03	b	h	
# ::edge	have-03	ARG1	clock	h	c	
# ::edge	clock	location	side	c	s	
# ::edge	tower	part	side	t2	s	
(t2 / tower
    :location (n / next-to
        :op1 (b / building
            :ARG0-of (h / have-03
                :ARG1 (c / clock
                    :location (s / side)))))
    :mod (t / tall
        :degree (v / very))
    :part s)
