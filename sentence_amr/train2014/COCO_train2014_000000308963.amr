# ::edge	person	ARG0-of	have-org-role-91	p2	h	
# ::edge	have-org-role-91	ARG2	umpire	h	u	
(a / and
    :op1 (b / bat-01
        :ARG0 (m / man))
    :op2 (b2 / behind
        :op1 m
        :op2 (p / person
            :ARG0-of (c / catch-01)))
    :op3 (p2 / person
        :ARG0-of (h / have-org-role-91
            :ARG2 (u / umpire))
        :location (b3 / behind
            :op1 p)))
