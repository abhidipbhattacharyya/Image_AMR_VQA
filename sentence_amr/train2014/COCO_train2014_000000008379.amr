(p2 / pole
    :ARG0-of (h2 / have-03
        :ARG1 (s / sign
            :ARG1-of (h / hang-01
                :ARG2 p2)
            :topic (p / park-01)))
    :ARG1-of (r / rust-01)
    :mod (o / old))


# ::tok Across from it is an old brick building with three windows .
