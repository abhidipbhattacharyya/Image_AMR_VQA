# ::tok Three person taking meals .
# ::node	0	3	0-1
# ::node	p	person	1-2
# ::node	t	take-01	2-3
# ::node	m	meal	3-4
# ::root	t	take-01
# ::edge	person	quant	3	p	0	
# ::edge	take-01	ARG0	person	t	p	
# ::edge	take-01	ARG1	meal	t	m	
(t / take-01
    :ARG0 (p / person
        :quant 3)
    :ARG1 (m / meal))


