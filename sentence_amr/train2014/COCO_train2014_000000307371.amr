# ::tok A tall brown clock tower with a clock mounted on each of it 's sides .
# ::node	t	tall	1-2
# ::node	b	brown	2-3
# ::node	c	clock	3-4
# ::node	t2	tower	4-5
# ::node	c2	clock	7-8
# ::node	m	mount-03	8-9
# ::node	e	each	10-11
# ::node	s	side	14-15
# ::root	t2	tower
# ::edge	tower	mod	clock	t2	c	
# ::edge	tower	mod	brown	t2	b	
# ::edge	tower	mod	tall	t2	t	
# ::edge	mount-03	ARG1	clock	m	c2	
# ::edge	tower	ARG0-of	mount-03	t2	m	
# ::edge	side	mod	each	s	e	
# ::edge	mount-03	location	side	m	s	
# ::edge	tower	part	side	t2	s	
(t2 / tower
    :ARG0-of (m / mount-03
        :ARG1 (c2 / clock)
        :location (s / side
            :mod (e / each)))
    :mod (c / clock)
    :mod (b / brown)
    :mod (t / tall)
    :part s)
