# ::tok A large clock tower with a clock mounted on it 's side .
# ::node	l	large	1-2
# ::node	c	clock	2-3
# ::node	t	tower	3-4
# ::node	c2	clock	6-7
# ::node	m	mount-03	7-8
# ::node	s	side	11-12
# ::root	t	tower
# ::edge	tower	mod	clock	t	c	
# ::edge	tower	mod	large	t	l	
# ::edge	mount-03	ARG1	clock	m	c2	
