# ::tok A clock tower of some sort as part of a building of some sort .
# ::node	c	clock	1-2
# ::node	t	tower	2-3
# ::node	s2	sort	5-6
# ::node	b	building	10-11
# ::node	s	some	12-13
# ::node	s3	sort	13-14
# ::root	t	tower
# ::edge	tower	mod	clock	t	c	
# ::edge	tower	mod	sort	t	s2	
# ::edge	tower	part-of	building	t	b	
# ::edge	sort	mod	some	s3	s	
# ::edge	building	mod	sort	b	s3	
(t / tower
    :mod (c / clock)
    :mod (s2 / sort)
    :part-of (b / building
        :mod (s3 / sort
            :mod (s / some))))
