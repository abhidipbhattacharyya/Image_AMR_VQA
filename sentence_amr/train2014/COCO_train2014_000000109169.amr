# ::tok A giant brick clock tower with a massive doing clock on each of it 's faces .
# ::node	g	giant	1-2
# ::node	b	brick	2-3
# ::node	c	clock	3-4
# ::node	t	tower	4-5
# ::node	h	have-03	5-6
# ::node	m	massive	7-8
# ::node	d	do-02	8-9
# ::node	c2	clock	9-10
# ::node	e	each	11-12
# ::node	f	face	15-16
# ::root	t	tower
# ::edge	tower	mod	clock	t	c	
# ::edge	tower	consist-of	brick	t	b	
# ::edge	tower	mod	giant	t	g	
# ::edge	tower	ARG0-of	have-03	t	h	
# ::edge	clock	ARG0-of	do-02	c2	d	
