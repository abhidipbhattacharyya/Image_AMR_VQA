# ::edge	tower	part	face	t	f	
(t / tower
    :ARG0-of (h / have-03
        :ARG1 (c2 / clock
            :ARG0-of (d / do-02)
            :location (f / face
                :mod (e / each))
            :mod (m / massive)))
    :consist-of (b / brick)
    :mod (c / clock)
    :mod (g / giant)
    :part f)
